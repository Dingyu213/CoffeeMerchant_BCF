                      name="SqlCommand"
                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=14.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">-- Script written by Xiao Li, Dingyu Liang, Siyu Pei
-- Script query to load the Fact Table
SELECT  
	dbo.[Order].OrderID AS OrderID_DD,
	dbo.Inventory.InventoryID,
	dbo.Employee.EmployeeID,
	dbo.State.StateID,
	dbo.[Order].OrderDate,
	dbo.[Order].ShipDate,
	dbo.OrderLine.Quantity AS Sales_Quantity,
	dbo.OrderLine.Price AS Full_Price,
	dbo.Inventory.Price AS Cost,
	dbo.OrderLine.Discount AS Discount,
	dbo.Inventory.OnHand AS Inventory_On_Hand,
	dbo.State.TaxRate AS Tax_Rate,
	Revenue = ([OrderLine].Price)*([OrderLine].Quantity)*([OrderLine].Discount),
	Profit = (([OrderLine].Price)*([OrderLine].Quantity)*([OrderLine].Discount))-(([Inventory].Price)*([OrderLine].Quantity)),
	Actual_Sale_Price = ([OrderLine].Price)*([OrderLine].Discount)
FROM CoffeeMerchant.dbo.[Order]
INNER JOIN CoffeeMerchant.dbo.Consumer 
	ON CoffeeMerchant.dbo.[Order].OrderID = CoffeeMerchant.dbo.Consumer.ConsumerID 
INNER JOIN CoffeeMerchant.dbo.State
	ON CoffeeMerchant.dbo.Consumer.State = CoffeeMerchant.dbo.State.StateID
INNER JOIN CoffeeMerchant.dbo.OrderLine
	ON CoffeeMerchant.dbo.[Order].OrderID = CoffeeMerchant.dbo.OrderLine.OrderID
INNER JOIN CoffeeMerchant.dbo.Inventory
	ON CoffeeMerchant.dbo.OrderLine.InventoryID = CoffeeMerchant.dbo.Inventory.InventoryID
INNER JOIN CoffeeMerchant.dbo.Employee
	ON CoffeeMerchant.dbo.[Order].EmployeeID = CoffeeMerchant.dbo.Employee.EmployeeID</property>
